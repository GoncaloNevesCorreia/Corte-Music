{
  "name": "discord-music-bot",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "clear": "rimraf ./build && rimraf ./bundle",
    "build": "npm run clear && tsc",
    "dev": "nodemon",
    "start": "npm run build && node .",
    "start-prod": "pm2 start npm --name \"discord-music-bot\" -- start",
    "deploy": "cross-env IS_SCRIPT=true ts-node src/scripts/deploy.ts",
    "deploy-prod": "cross-env NODE_ENV=production npm run deploy",
    "bundle": "node esbuild.config.js",
    "lint": "eslint src/**/*.{js,ts} --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.16",
    "eslint": "^8.36.0",
    "nodemon": "^2.0.21",
    "pm2": "^5.3.0",
    "rimraf": "^4.4.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@discord-player/extractor": "^4.2.1",
    "@discordjs/opus": "^0.9.0",
    "discord-player": "^6.1.1",
    "discord.js": "^14.9.0",
    "dotenv": "^16.0.3",
    "ffmpeg-static": "^5.1.0",
    "ytdl-core": "^4.11.4"
  }
